{
  "compilerOptions": {
    "module": "ES6",
    "moduleResolution": "node",
    "lib": [
      "es6"
    ],
    "outDir": "./dist/",
    "noImplicitAny": true,
    "noImplicitThis": true,
    "emitDeclarationOnly": true,
    "strict": true,
    "types": [],
    "declaration": true,
    "forceConsistentCasingInFileNames": true,
    "rootDir": "./src"
  },
  "include": [
    "src/",
  ],
}
